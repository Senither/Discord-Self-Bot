{
    "name": "Watchdog",
    "version": "0.2.0",
    "description": "A simple Discord self-bot written for the AvaIre Discord bot.",
    "main": "start.js",
    "maintainers": [
        {
            "name": "Alexis Tan",
            "email": "alexis@sen-dev.com"
        }
    ],
    "scripts": {
        "start": "node start.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "engines": {
        "node": "6.x.x - 7.x.x",
        "npm": ">=3.x.x"
    },
    "xo": {
        "space": 4,
        "semicolon": true,
        "globals": [
            "app",
            "bot"
        ],
        "rules": {
            "camelcase": 0,
            "new-parens": 0,
            "guard-for-in": 0,
            "prefer-const": 0,
            "no-eval": 0,
            "no-unused-vars": 0,
            "no-prototype-builtins": 0,
            "unicorn/filename-case": 0,
            "import/no-dynamic-require": 0
        }
    },
    "license": "MIT",
    "dependencies": {
        "chalk": "^2.0.1",
        "discordie": "^0.11.0",
        "lodash": "^4.17.4",
        "moment": "^2.18.1",
        "require-directory": "^2.1.1"
    },
    "devDependencies": {
        "xo": "^0.18.0"
    }
}
